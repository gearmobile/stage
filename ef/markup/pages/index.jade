doctype html
html(class='no-js', lang='ru')

    include ../components/head/head
    include ../components/block-topheader/block-topheader
    include ../components/block-hero/block-hero
    include ../components/block-about/block-about
    include ../components/block-skills/block-skills
    include ../components/block-team/block-team
    include ../components/block-services/block-services
    include ../components/block-facts/block-facts
    include ../components/block-work/block-work
    include ../components/block-recent/block-recent
    include ../components/block-follow/block-follow
    include ../components/block-testimonials/block-testimonials
    include ../components/block-pricing/block-pricing
    include ../components/block-citation/block-citation
    include ../components/block-touch/block-touch
    include ../components/block-map/block-map
    include ../components/footer/footer

    head
        +head(head.defaults)
    body.page
        |%=symbols=%

        //+block-topheader()

        section.page__wrapper

            +block-hero()
            +block-about()
            +block-skills()
            +block-team()
            +block-services()
            +block-facts()
            +block-work()
            +block-recent()
            +block-follow()
            +block-testimonials()
            +block-pricing()
            +block-citation()
            +block-touch()
            //+block-map()

            .page__buffer

        .page__footer
            +footer()

        script(src='%=static=%js/separate-js/svg4everybody.min.js')
        script svg4everybody();

        //- Main scripts. You can replace it, but I recommend you to leave it here
        script(src='%=static=%js/main%=hash=%%=min=%.js')
