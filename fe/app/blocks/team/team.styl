.team
	min-height 108.2rem
	background #f7f7f7 url(../images/bg-team.png) 50% 50% no-repeat
	background-size cover
	overflow hidden
	position relative
	padding 26rem 0 38rem

	&::before
		content ''
		display block
		width 100%
		height 16rem
		background-color inherit
		position absolute
		right 0
		top 0
		transform-origin 0 100%
		transform skewY(-4.7deg)

	&__center
		lost-center $width

		& > .heading .heading__title > span,
		& > .heading .heading__subtitle,
		& > .heading .heading__text
			color $white


// GALLERY
// ----------------------------------------

.gallery
	lost-center 100%
	margin-top 10rem
	padding 0 10px

	&__item
		lost-column 3/12
		lost-gutter 2rem
		text-align center

	&__image
		size(17.8rem)
		border-radius 50%
		margin 0 auto 3.6rem
		cursor pointer
		position relative

		&::before
			content ''
			display block
			width 100%
			height 100%
			border-radius 50%
			position absolute
			top 0
			left 0
			z-index 1
			transition background-color .2s

		&:hover::before
			background-color rgba(0, 0, 0, .9)

		& > img
			display block
			width 100%
			height auto
			border-radius 50%

	&__title
		color $white
		font-size 2rem
		font-weight 700
		text-transform uppercase
		margin-bottom .7rem

	&__subtitle
		color #00e4ff
		font-size 1.2rem
		text-transform capitalize
		margin-bottom 1.8rem

	&__text
		color $white
		font-size 1.2rem
		line-height 1.4


// SOCIAL ICONS
// -------------------------------------------

