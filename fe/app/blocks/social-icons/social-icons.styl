.social
	display flex
	justify-content center
	position absolute
	top 50%
	left 50%
	transform translate(-50%, -50%)
	z-index 1
	animation-delay .1s

	&__item
		display block
		size(1.8rem)
		background-position center center
		background-repeat no-repeat
		background-size cover
