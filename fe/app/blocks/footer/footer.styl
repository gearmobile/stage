	// FOOTER COPYRIGHT
	// -------------------------------------------------------

	&__copyright
		padding 3rem 0 3.5rem
		text-align center
		color $white
		background-color $blue
		font-weight 700

	&__link
	&__author
		text-decoration none
		color #0f91b1
		transition color .2s

		&:hover
		&:active
			color $orange
